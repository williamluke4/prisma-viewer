{
  "name": "prisma-viewer",
  "version": "1.0.4",
  "description": "Displays Prisma Datamodels/Schemas",
  "main": "index.js",
  "scripts": {
    "build:cli": "tsc -d",
    "build:web": "parcel build src/index.ts -d public",
    "build": "npm run build:cli && npm run build:web",
    "dev:web": "parcel watch src/index.ts -d public",
    "dev:cli": "ts-node ./src/bin.ts",
    "dev": "npm run dev:web & npm run dev:cli",
    "release": "auto shipit"
  },
  "bin": {
    "pv": "dist/bin.js"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "dependencies": {
    "@prisma/sdk": "0.0.204",
    "arg": "^4.1.2",
    "chalk": "^2.4.2",
    "dagre": "^0.8.5",
    "express": "^4.17.1",
    "fastify": "^2.12.0",
    "fastify-static": "^2.6.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "prisma2": "^2.0.0-preview021",
    "strip-indent": "^3.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^9.10.6",
    "auto": "^9.10.6",
    "@types/dagre": "^0.7.42",
    "@types/lodash": "^4.14.149",
    "@zeit/ncc": "^0.20.5",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.3"
  }
}
